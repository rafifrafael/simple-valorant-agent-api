{
    "version": 2,
    "builds": {
      "source": "api/index.php",
      "destination": "public/index.php"
    },
    "rewrites": [
      { "source": "/(.*)", "destination": "/api/index.php" }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://valorant-agent.vercel.app/",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    }
  }
  